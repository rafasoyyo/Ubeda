{
  "name": "wedding",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=develop&& nodemon -e jade,js --inspect ./bin/www",
    "start": "set NODE_ENV=production&& node ./bin/www"
  },
  "engine": {
    "node": "10.15.0"
  },
  "nodemonConfig": {
    "ext": "routes/*.js, views/*.jade",
    "verbose": true
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.1",
    "telegram-api": "^4.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
